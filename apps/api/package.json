{
  "name": "api",
  "version": "1.0.0",
  "description": "server for questionnaire",
  "main": "build/src/server.js",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc --outDir build --experimentalDecorators && npm run generate:node-client",
    "start": "node build/src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:node-client": "npx openapi-typescript-codegen -i ./build/swagger.json -o ../../packages/node-client/ -c axios"
  },
  "author": "Ankish Nayak",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "cookie-parser": "^1.4.6",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "tsoa": "^5.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^5.3.1",
    "@types/api-error-handler": "^1.0.34",
    "@types/cookie-parser": "^1.4.4",
    "@types/cookies": "^0.7.8",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.6",
    "@types/swagger-ui-express": "^4.1.4",
    "api-error-handler": "^1.0.0",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "prisma": "*",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "types": "*",
    "typescript": "^5.2.2",
    "node-client": "*"
  }
}
